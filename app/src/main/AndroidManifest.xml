<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jmnl2020.attendanceapp3">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".myApp"
        android:allowBackup="true"
        android:icon="@drawable/iconimg"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconimg"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        >
        <activity android:name=".LeavingStudentActivity"></activity>
        <activity android:name=".StudentInfoActivity" />
        <activity android:name=".StudentEditActivity" />
        <activity
            android:name=".NumberInputActivity"
            android:label="@string/student_number_input"
            android:theme="@style/customTheme" />
        <activity android:name=".SelectionActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- 카카오 네이티브 앱 키 등록 -->
        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-1632314811563134~3183633822"/>
    </application>

</manifest>